@model IEnumerable<sep6.Models.Member>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MemberId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MemberName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccOpenDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(model => item.MemberId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccOpenDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MemberId }) |
            @Html.ActionLink("Details", "Details", new { id = item.MemberId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MemberId })
        </td>
    </tr>
}

</table>
